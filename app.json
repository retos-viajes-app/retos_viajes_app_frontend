      // Emulador android rafa:
      "API_URL_ANDROID": "http://10.0.2.2:13000",
      // Ordenador pablo:
      //"API_URL_ANDROID": "http://192.168.1.111:13000",
      "API_URL_IOS": "http://192.168.1.111:13000"
    }
  }
